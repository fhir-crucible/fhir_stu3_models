			<xs:attribute name="readonly">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="readonly"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="onselect" type="Script"/>
			<xs:attribute name="onchange" type="Script"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="fieldset">
		
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="legend"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:choice>
						<xs:element ref="p"/>
						<xs:choice>
							<xs:element ref="h1"/>
							<xs:element ref="h2"/>
							<xs:element ref="h3"/>
							<xs:element ref="h4"/>
							<xs:element ref="h5"/>
							<xs:element ref="h6"/>
						</xs:choice>
						<xs:element ref="div"/>
						<xs:choice>
							<xs:element ref="ul"/>
							<xs:element ref="ol"/>
							<xs:element ref="dl"/>
						</xs:choice>
						<xs:choice>
							<xs:element ref="pre"/>
							<xs:element ref="hr"/>
							<xs:element ref="blockquote"/>
							<xs:element ref="address"/>
						</xs:choice>
						<xs:element ref="fieldset"/>
						<xs:element ref="table"/>
